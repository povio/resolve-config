{
  "name": "@povio/resolve-config",
  "version": "1.0.9",
  "bin": "dist/sh.js",
  "scripts": {
    "start": "tsx ./src/sh.ts",
    "start:dist": "node ./dist/sh.js",
    "test": "tsx --test ./src/**/*.test.ts",
    "test:e2e": "tsx --test ./test/**/*.test.ts",
    "test:watch": "tsx --watch --test ./src/**/*.test.ts",
    "build:clean": "rm -rf ./dist",
    "build": "yarn build:clean && tsup",
    "build:check": "yarn tsc --project . --noEmit",
    "lint": "eslint --fix && prettier --write .",
    "push": "yarn exec ./scripts/publish.sh"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "homepage": "https://github.com/povio/resolve-config",
  "bugs": "https://github.com/povio/resolve-config/issues",
  "repository": {
    "url": "git+https://github.com/povio/resolve-config.git",
    "type": "git"
  },
  "author": "Marko Zabreznik <marko.zabreznik@povio.com>",
  "license": "BSD-3-Clause",
  "peerDependencies": {
    "@aws-sdk/client-ssm": "^3.840.0",
    "@aws-sdk/client-sts": "^3.840.0",
    "@aws-sdk/credential-providers": "^3.840.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-ssm": {
      "optional": true
    },
    "@aws-sdk/client-sts": {
      "optional": true
    },
    "@aws-sdk/credential-providers": {
      "optional": true
    }
  },
  "devDependencies": {
    "@aws-sdk/client-ssm": "^3.864.0",
    "@aws-sdk/client-sts": "^3.864.0",
    "@aws-sdk/credential-providers": "^3.864.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@swc/core": "^1.13.3",
    "@types/node": "^24.3.0",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "deepmerge": "^4.3.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "yaml": "^2.8.1",
    "zod": "^4.0.17"
  },
  "engines": {
    "node": ">= 22",
    "yarn": ">= 4",
    "npm": ">= 8"
  },
  "packageManager": "yarn@4.9.2"
}
